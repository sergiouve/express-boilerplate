{
    "name": "express-boilerplate",
    "version": "0.0.1",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.15.0",
        "cors": "^2.8.4",
        "dotenv": "^6.0.0",
        "eslint": "^5.4.0",
        "express": "^4.13.4",
        "express-session": "^1.13.0",
        "jsonwebtoken": "^8.3.0",
        "lodash": "^4.17.10",
        "mysql2": "^1.5.3",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "sequelize": "^4.38.0",
        "winston": "^2.2.0"
    },
    "engines": {
        "node": "8.11.3"
    },
    "scripts": {
        "lint": "node node_modules/eslint/bin/eslint --ignore-path .gitignore .",
        "test": "NODE_ENV=test mocha \"tests/**/*.js\" --exit --timeout 10000",
        "start": "npm run lint && node index.js",
        "watch": "nodemon index.js",
        "migrations:rollback": "node_modules/.bin/sequelize db:migrate:undo:all",
        "migrations": "node_modules/.bin/sequelize db:migrate",
        "seeders": "node_modules/.bin/sequelize db:seed:all"
    },
    "devDependencies": {
        "assert": "^1.3.0",
        "chai": "^4.1.2",
        "chai-http": "^4.0.0",
        "mocha": "^5.2.0",
        "sequelize-cli": "^4.1.1",
        "should": "^8.4.0",
        "sqlite3": "^4.0.6"
    }
}
